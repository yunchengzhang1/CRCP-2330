/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel, out=NOTsel);
    And(a=a, b=NOTsel, out=aR);
    And(a=b, b=sel, out=bR);
    Or(a=aR, b=bR, out=out); 

}